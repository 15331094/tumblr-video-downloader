{
	"name": "__MSG_appName__",
	"version": "1.0",
	"default_locale": "zh_CN",
	"description":"__MSG_description__",
	"background": {
		"scripts": ["background.js"]
	},
	"content_scripts": [
		{
			"matches": ["*://*.tumblr.com/*"],
			"css":["style.css"],
			"js": ["jquery.min.js","main.js"],
			"all_frames": true,
			"run_at":"document_end"
		}
	],
	"permissions": [
        "contextMenus","*://*.tumblr.com/*"
	],
	"icons": {
		"16": "icon_16.png",
		"48": "icon_48.png",
		"128": "icon_128.png"
	},
	"browser_action": {
		"name": "__MSG_appName__",
		"default_icon": "icon_128.png"
	},
	"manifest_version": 2
}
